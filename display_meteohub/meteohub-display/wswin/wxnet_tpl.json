<!-- %customfile=wxnet.json% -->
%unit_off%%alwaysseppoint_on%{
	"header": {
		"generator": "wswin",
		"version": "1.0"
	},
	"current": {
		"dt": {
			"utc": "%ws_utc_year%-%ws_utc_month%-%ws_utc_day%-%ws_utc_hour%-%ws_utc_minute%",
			"local": "%ws_year%-%ws_month%-%ws_day%-%ws_hour%-%ws_minute%"
		},
		"temperature": {
			"v": %curval[2]%
		},
		"dewpoint": {
			"v": %curval[43]%
		},
		"pressure": {
			"v": %curval[33]%
		},
		"humidity": {
			"v": %curval[18]%
		},
		"precipitation": [{
			"v": %rainrate%,
			"p": "rate"
		},
		{
			"v": %rain1h%,
			"p": "last60m"
		}],
		"wind": {
			"speed": {
				"v": %curval[35]%
			},
			"gust": {
				"v": %avg10minwindg%
			},
			"direction": {
				"v": %curval[36]%
			}
		}
	}
}%unit_on%%alwaysseppoint_off%